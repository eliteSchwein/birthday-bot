{
    "name": "birthday-bot",
    "version": "0.0.3",
    "description": "Birthday Bot based on Discord.js",
    "main": "index.js",
    "scripts": {
        "typeorm": "typeorm-ts-node-commonjs",
        "start": "node --max-old-space-size=64 --expose-gc dist/src/App.js",
        "debugstart": "node --inspect=0.0.0.0:3000 --max-old-space-size=64 --trace_gc --expose-gc --trace-deprecation --trace-warnings --trace-uncaught --track-heap-objects dist/src/App.js",
        "checkcodestyle": "npx eslint ./**",
        "autofixcodestyle": "npx eslint ./** --fix",
        "ncu": "ncu",
        "generatelocale": "node src/helper/LocaleGenerationHelper.js",
        "build": "tsc --build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/eliteSchwein/mooncord.git"
    },
    "keywords": [],
    "author": "eliteSCHW31N",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/eliteSchwein/mooncord/issues"
    },
    "homepage": "https://github.com/eliteSchwein/mooncord#readme",
    "devDependencies": {
        "@discordjs/rest": "^2.5.0",
        "@types/node": "^22.15.18",
        "@types/readline-sync": "^1.4.8",
        "async-wait-until": "2.0.27",
        "axios": "^1.9.0",
        "bytes": "^3.1.2",
        "esbuild": "^0.25.4",
        "eslint": "^9.26.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^7.2.1",
        "form-data": "^4.0.2",
        "js-conf-parser": "^1.0.3",
        "lodash": "^4.17.21",
        "node-fetch": "^3.3.2",
        "npm-check-updates": "^18.0.1",
        "readline-sync": "^1.4.10",
        "regex-parser": "^2.3.1",
        "stacktrace-js": "^2.0.2",
        "ts-node": "^10.9.2",
        "typeorm": "^0.3.24",
        "typescript": "^5.8.3",
        "websocket-ts": "^2.2.1",
        "ws": "^8.18.2"
    },
    "dependencies": {
        "discord.js": "^14.19.3",
        "reflect-metadata": "^0.2.2",
        "sharp": "^0.34.1"
    }
}
