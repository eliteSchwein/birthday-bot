{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
  },
  "include": [
    "./src/"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
